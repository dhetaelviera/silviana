	<queryString language="SQL">
		<![CDATA[SELECT
     transaksi.`idTransaksi` AS ID_Transaksi,
     member.`nama` AS nama,
     transaksi.`tanggalBeli` AS tanggalBeli,
     transaksi.`pegawai` AS pegawai,
     kurir.`nama` AS transaksi_kurir,
     transaksi.`invoice` AS transaksi_invoice
     
FROM
     `transaksi` transaksi join
     `member` member on member.`idMember`= transaksi.`idMember`  join
     `kurir` kurir on kurir.`idKurir`=transaksi.`kurir`]]>
